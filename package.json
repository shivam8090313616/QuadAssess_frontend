{
  "name": "QuadAssess",
  "version": "1.1.2",
  "description": "React version of Argon Design System by Creative Tim",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env PUBLIC_URL=/ react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "sass src/assets/scss/argon-design-system-react.scss src/assets/css/argon-design-system-react.css",
    "minify-sass": "sass src/assets/scss/argon-design-system-react.scss src/assets/css/argon-design-system-react.min.css --style compressed"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@yaireo/tagify": "^4.31.6",
    "ajv": "^8.16.0",
    "axios": "^1.7.7",
    "bootstrap": "^4.6.2",
    "chart.js": "^4.4.6",
    "classnames": "2.3.2",
    "framer-motion": "^11.11.11",
    "gsap": "^3.12.5",
    "headroom.js": "0.12.0",
    "lottie-react": "^2.4.0",
    "moment": "2.29.4",
    "nouislider": "15.4.0",
    "react": ">=16.0.0",
    "react-bootstrap": "^2.10.5",
    "react-chartjs-2": "^5.2.0",
    "react-datetime": "3.2.0",
    "react-dom": ">=16.0.0",
    "react-icons": "^5.3.0",
    "react-lottie": "^1.2.7",
    "react-router-dom": "^6.11.1",
    "react-scripts": "5.0.1",
    "react-select": "^5.8.2",
    "react-toastify": "^10.0.6",
    "reactstrap": "^8.10.0",
    "sass": "1.62.1",
    "styled-component": "^2.8.0",
    "sweetalert2": "^11.14.5"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "@types/markerclustererplus": "2.1.33",
    "@types/react": "18.2.6",
    "cross-env": "^7.0.3",
    "eslint-plugin-flowtype": "8.0.3",
    "jquery": "3.7.0",
    "typescript": "5.0.4"
  },
  "overrides": {
    "svgo": "3.0.2"
  }
}
